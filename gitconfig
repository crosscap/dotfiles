	br = branch
	ci = commit
	st = status -s
	last = log -1 HEAD
	graph = log --all --graph --decorate --oneline
	recommit = commit --amend --no-edit
	unstage = reset HEAD --
[core]
	excludesfile = /home/crosscap/.gitignore_global
	editor = vim
	autocrlf = input
[pager]
	branch = false
[diff]
	tool = vimdiff
[color]
	ui = auto
[pull]
	rebase = false
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
